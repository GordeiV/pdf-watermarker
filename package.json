{
  "name": "pdfwatermarker",
  "version": "1.0.0",
  "description": "Web Service for adding watermarks to PDF Documents",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
