    "express": "^4.18.2",
    "express-fileupload": "^1.4.1",
    "express-validator": "^7.0.1",
    "nodemon": "^3.0.1",
    "pdf-lib": "^1.17.1",
    "uuid": "^9.0.1"
  }
}
